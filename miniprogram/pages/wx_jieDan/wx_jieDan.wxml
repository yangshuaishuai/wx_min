<view class='weui-cells__title'>工单编号： {{gdId}}</view>
<view class="weui-form-preview">
  <view class="weui-form-preview__hd">
    <view class="weui-form-preview__item">
      <view class="weui-form-preview__label">状态</view>
      <view class="weui-form-preview__value_in-hd">{{status}}</view>
    </view>
  </view>
  <view class="weui-form-preview__bd">
    <view class="weui-form-preview__item">
      <view class="weui-form-preview__label">报修人姓名</view>
      <view class="weui-form-preview__value">{{bx_name}}</view>
    </view>
    <view class="weui-form-preview__item">
      <view class="weui-form-preview__label">手机号码</view>
      <view class="weui-form-preview__value">{{bx_phone}}</view>
    </view>
    <view class="weui-form-preview__item">
      <view class="weui-form-preview__label">设备名称</view>
      <view class="weui-form-preview__value">{{sb_name}}</view>
    </view>
    <view class="weui-form-preview__item">
      <view class="weui-form-preview__label">设备型号</view>
      <view class="weui-form-preview__value">{{sb_xh}}</view>
    </view>
    <view class="weui-form-preview__item">
      <view class="weui-form-preview__label">故障图片</view>
      <view class="weui-form-preview__value" style='color:#0bb20c'>点击查看</view>
    </view>
    <view class="weui-form-preview__item">
      <view class="weui-form-preview__label">故障描述</view>
      <view class="weui-form-preview__value">{{sb_ms}}</view>
    </view>
    <view class="weui-form-preview__item">
      <view class="weui-form-preview__label">设备厂商</view>
      <view class="weui-form-preview__value">{{sbCs}}</view>
    </view>
    <view class="weui-form-preview__item">
      <view class="weui-form-preview__label">使用机构</view>
      <view class="weui-form-preview__value">{{orgName}}</view>
    </view>
    <view class="weui-form-preview__item">
      <view class="weui-form-preview__label">使用人</view>
      <view class="weui-form-preview__value">{{useName}}</view>
    </view>
    <view class="weui-form-preview__item">
      <view class="weui-form-preview__label">报修时间</view>
      <view class="weui-form-preview__value">{{bxTime}}</view>
    </view>
  </view>
  <view class="weui-form-preview__ft">
    <navigator url="" class="weui-form-preview__btn weui-form-preview__btn_default" 
    hover-class="weui-form-preview__btn_active">退回</navigator>
    <navigator url="" class="weui-form-preview__btn weui-form-preview__btn_primary" 
    hover-class="weui-form-preview__btn_active">接单</navigator>
  </view>
</view>